{
  "name": "DeepSleep",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "build:android": "cd android && ./gradlew assembleRelease",
    "clean:android": "cd android && ./gradlew clean",
    "build:ios": "cd ios && xcodebuild -workspace DeepSleep.xcworkspace -scheme DeepSleep -configuration Release -sdk iphoneos -derivedDataPath build",
    "clean:ios": "cd ios && xcodebuild clean -workspace DeepSleep.xcworkspace -scheme DeepSleep -configuration Release -sdk iphoneos -derivedDataPath build",
    "install:android": "adb install -r android/app/build/outputs/apk/release/app-release.apk",
    "install:ios": "xcrun simctl install booted ios/build/Release-iphonesimulator/DeepSleep.app"
  },
  "dependencies": {
