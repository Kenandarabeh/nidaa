{
  "name": "DeepSleep",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "build:android": "cd android && ./gradlew assembleRelease",
    "clean:android": "cd android && ./gradlew clean",
    "build:ios": "cd ios && xcodebuild -workspace DeepSleep.xcworkspace -scheme DeepSleep -configuration Release -sdk iphoneos -derivedDataPath build",
    "clean:ios": "cd ios && xcodebuild clean -workspace DeepSleep.xcworkspace -scheme DeepSleep -configuration Release -sdk iphoneos -derivedDataPath build",
    "install:android": "adb install -r android/app/build/outputs/apk/release/app-release.apk",
    "install:ios": "xcrun simctl install booted ios/build/Release-iphonesimulator/DeepSleep.app"
  },
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.25.9",
    "@formatjs/intl-getcanonicallocales": "^2.5.1",
    "@formatjs/intl-locale": "^4.2.1",
    "@formatjs/intl-pluralrules": "^5.3.1",
    "@react-native-async-storage/async-storage": "^2.0.0",
    "@react-native-community/template": "0.75.4",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.11.0",
    "@react-navigation/stack": "^6.4.1",
    "@reduxjs/toolkit": "^2.3.0",
    "axios": "^1.7.7",
    "i18next": "^23.16.3",
    "lottie-react-native": "^7.0.0",
    "nativewind": "^4.1.21",
    "react": "18.3.1",
    "react-i18next": "^15.1.0",
    "react-native": "0.75.4",
    "react-native-asset": "2.1.1",
    "react-native-dropdown-picker": "^5.4.6",
    "react-native-eject": "^1.0.0",
    "react-native-element-dropdown": "^2.12.2",
    "react-native-gesture-handler": "^2.20.2",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-localize": "^3.2.1",
    "react-native-safe-area-context": "^4.11.1",
    "react-native-screens": "^3.35.0",
    "react-native-size-matters": "^0.4.2",
    "react-native-svg": "^15.8.0",
    "react-native-vector-icons": "^10.2.0",
    "react-native-video": "^6.7.0",
    "react-redux": "^9.1.2",
    "redux": "^5.0.1",
    "tailwindcss": "^3.4.14",
    "zustand": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.75.4",
    "@react-native/eslint-config": "0.75.4",
    "@react-native/metro-config": "0.75.4",
    "@react-native/typescript-config": "0.75.4",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "18.3.1",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
